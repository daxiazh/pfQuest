{
  "name": "pfquest-data-merger",
  "version": "1.0.0",
  "description": "Merge pfQuest base data with Turtle WoW custom data and extract valid quest IDs",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "merge": "node quest-merger.js",
    "scrape": "node scrape-quest-rewards-selenium.js",
    "scrape-debug": "node scrape-quest-rewards-selenium.js --debug",
    "scrape-sample": "node scrape-quest-rewards-selenium.js -c 5",
    "scrape-headless": "node scrape-quest-rewards-selenium.js --headless -c 10",
    "scrape-retry": "node scrape-quest-rewards-selenium.js --retry-failed",
    "scrape-all": "node scrape-quest-rewards-selenium.js --all",
    "scrape-all-headless": "node scrape-quest-rewards-selenium.js --all --headless",
    "convert": "node convert-quest-rewards-to-lua.js",
    "test-conversion": "node test-conversion.js",
    "test": "node test.js",
    "test-incremental": "node test-incremental.js"
  },
  "keywords": ["wow", "turtle-wow", "pfquest", "data-merger"],
  "author": "pfQuest Tools",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "selenium-webdriver": "^4.15.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  }
}